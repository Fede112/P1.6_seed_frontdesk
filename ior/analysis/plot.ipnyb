   "source": [
    "mpiio, ncmpi, posix, hdf5 = np.loadtxt('write_plot.txt', delimiter = ' ', unpack = True, skiprows = 1)\n",
    "\n",
    "#print(mpiio)\n",
    "#print(ncmpi)\n",
    "#print(posix)\n",
    "#print(hdf5)\n",
    "\n",
    "index = np.arange(3)\n",
    "x_axis = [\"lustre\",\"NFS\",\"local\"]\n",
    "\n",
    "width = 0.1\n",
    "\n",
    "plt.figure(figsize=(15,8))\n",
    "\n",
    "\n",
    "p1 = plt.bar(index - width/2, mpiio, width)\n",
    "p2 = plt.bar(index - width*3/2, posix, width)\n",
    "p3 = plt.bar(index + width/2, ncmpi, width)\n",
    "p4 = plt.bar(index + width*3/2, hdf5, width)\n",
    "\n",
    "plt.title('IOR BENCHMARK - WRITE', fontsize=24)\n",
    "plt.xticks(index, x_axis, fontsize=22)\n",
    "plt.ylabel('bandwidth [MiB]', fontsize=22)\n",
    "plt.legend((p1[0], p2[0], p3[0], p4[0]), ('mpiio', 'posix', 'ncmpi', 'hdf5'), fontsize=20) #, loc='lower righ'\n",
    "plt.savefig(\"./np4_blocksize1GiB/first_write_ior.png\", bbox_inches='tight')"
   ]
